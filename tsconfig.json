{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": ["ES2021"],
    "types": ["node", "jest"],
    "strict": true,
    "noEmit": true,
    "downlevelIteration": true,
    "noUncheckedIndexedAccess": true,
  },
  "exclude": ["node_modules"],
}
